<?xml version="1.0"?>
<package format="3">
  <name>g_compensation</name>
  <version>0.0.0</version>
  <description>Gravity compensation for FT-sensors.</description>

  <maintainer email="phg@ipa.fhg.de">Philipp Gorczak</maintainer>

  <license>TODO</license>

  <buildtool_depend>catkin</buildtool_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>std_srvs</exec_depend>
  <exec_depend condition="$ROS_DISTRO == melodic">python_orocos_kdl</exec_depend>
